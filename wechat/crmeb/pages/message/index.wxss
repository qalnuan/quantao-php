page{position: relative;min-height: 100%; background-image: url(data:image/jpeg;base64,);background-size: 100% auto; background-repeat: repeat;}

.container{padding: 0 24rpx;}
.title{width: 100%; padding:35rpx 0; border-bottom: 1px dashed #b1b1b1; line-height: 1.5; font-size: 36rpx; color: #2a1505;}
.title .time{margin-top: 10rpx; font-size: 28rpx; color: #888888;}
.title .time text:first-child{margin-right:30rpx;}
.con-box{width: 100%; padding:35rpx 0;  line-height: 1.5; color: #5b2d0c; font-size: 30rpx;}
.con-box image{width: 100%;display:block;}

