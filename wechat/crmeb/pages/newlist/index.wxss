page{background-color: #ffffff;}
.nav{width:100%;height:100rpx;display:flex;flex-direction:row; border-bottom:1px solid #eeeeee; -webkit-justify-content:center; justify-content:center; color:#333;}
.view-txt{width: 210rpx; line-height:89rpx;text-align:center;color:#333;}
.red{color: #ff0000; opacity: 1; border-bottom: 2px solid #c19157;}
.news-list .news-item{padding: 30rpx 24rpx; border-bottom: 1px solid #ccc;}
.news-list .news-item .picture{width: 200rpx; height: 170rpx;}
.news-list .news-item .picture image{width: 100%; height: 100%; border-radius: 3px;}
.news-list .news-item .pro-txt{margin-left: 24rpx; width: 470rpx; line-height: 1.4;}
.news-list .news-item .pro-txt text{display: block;}
.news-list .news-item .pro-txt .tit{overflow: hidden;white-space: nowrap;text-overflow: ellipsis; color: #000000;}
.news-list .news-item .pro-txt .description{margin:4rpx 0; font-size: 30rpx; color: #676767;display: -webkit-box; display:-moz-box; display:box;-webkit-box-orient: vertical; -moz-box-orient: vertical; -webkit-line-clamp: 2;-moz-line-clamp: 2;overflow: hidden;}
.news-list .news-item .pro-txt .time-wrapper{font-size: 22rpx; color: #999999;}
.news-list .news-item .pro-txt .time-wrapper view{height: 46.8rpx; line-height: 46.8rpx; padding-left: 25rpx;}
.news-list .news-item .pro-txt .time-wrapper .time{margin-right: 12rpx; background-image: url("data:image/png;base64,"); background-repeat: no-repeat; background-position: left center; background-size:21rpx;}
.news-list .news-item .pro-txt .time-wrapper .count{background-image: url("data:image/png;base64,"); background-repeat: no-repeat; background-position: left center; background-size:21rpx;}


