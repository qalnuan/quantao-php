/**index.wxss**/
.index-bg{width: 100%; height:376rpx;position:relative;}
.index-bg .search{position:absolute;width:690rpx;height:60rpx;border-radius:50rpx;background-color:rgba(255, 255, 255, 0.4);align-items:center;padding:0 20rpx;justify-content:space-between;top:25rpx;left:50%;margin-left:-345rpx;box-sizing:border-box;}
.index-bg .search .placeholder{font-size:26rpx;color:#fff;}
.index-bg .search .iconfont{font-size:35rpx;color:#fff;}
.index-bg .search input{width:595rpx;}
.index-bg swiper{width:100%; height:100%; position: relative;}
.index-bg .slide-image{width: 100%; height: 100%;}
.nav{width: 100%;box-sizing: border-box; height:190rpx; align-items: center; background-color: #fff;}
.nav .nav-item{width:25%;}
.nav .nav-item image{width:96rpx; height:96rpx;}
.nav .nav-item navigator{width: 100%;flex-direction: column; align-items: center;}
.nav .nav-item .nav-title{margin-top:15rpx;font-size: 24rpx;color:#333333;}
.new,.second-kill,.buying-goods{background-color:#fff;margin-top:15rpx;}
.new .title,.second-kill .title,.buying-goods .title{padding:0 20rpx;height:92rpx;text-align:center;font-size:26rpx;color:#282828;font-weight:bold;line-height:92rpx;position:relative;}
.new .title .icon-biaoqian1,.second-kill .title .icon-miaosha,.buying-goods .title .icon-tuangoujie{width:36rpx;height:36rpx;border-radius:100%;border:2rpx solid #f17e7e;font-size:24rpx;display:inline-block;text-align:center;line-height:36rpx;color:#f17e7e;margin-right:17rpx;}
.new .title .more,.second-kill .title .more,.buying-goods .title .more{position:absolute;right:20rpx;top:0;font-size:24rpx;color:#999999;align-items:center;font-weight:normal;}
.new .title .icon-youjian,.second-kill .title .icon-youjian,.buying-goods .title .icon-youjian{font-size:24rpx;margin-left:11rpx;}
.new .picture,.second-kill .picture,.buying-goods .picture{width:100%;height:240rpx;}
.new .picture image,.second-kill .picture image,.buying-goods .picture image{width:100%;height:100%;}
.new .scroll-product{width: 100%; overflow: hidden; white-space: nowrap; padding:20rpx 10rpx 28rpx 10rpx;box-sizing: border-box;}
.new .scroll-product .item{width:240rpx;margin-left:20rpx; display: inline-block;}
.new .scroll-product .item navigator{flex-direction: column;}
.new .scroll-product .item .img-box{width: 100%; height:240rpx;position:relative;}
.new .scroll-product .item .img-box .label{position:absolute;top:20rpx;left:-6rpx;height:36rox;width:112rpx;line-height:36rpx;font-size:20rpx;color:#fff;border-radius:0 50rpx 50rpx 0;text-align:center;background-color:#f17e7e;}
.new .scroll-product .item .img-box image{width: 100%; height:100%;border-radius:10rpx;}
.new .scroll-product .item .pro-info{width: 100%;font-size: 28rpx;}
.new .scroll-product .item .pro-info .pro-title{font-size:24rpx;color:#333333;margin-top:16rpx;}
.new .scroll-product .item .pro-info .price-small{color:#ff4020;margin-top:6rpx;}
.second-kill .title .icon-miaosha{border-color:#ecc344;color:#ecc344;}
.second-kill .kill-list{padding:0 30rpx;width:100%;box-sizing:border-box;}
.second-kill .kill-item{justify-content:space-between;align-items:center;}
.second-kill .kill-item image{width:200rpx;height:200rpx;border-radius:10rpx;}
.second-kill .kill-item .text{width:460rpx;}
.new .scroll-product .item .pro-info .price-small{color:#ff4020;margin-top:6rpx;}
.second-kill .title .icon-miaosha{border-color:#ecc344;color:#ecc344;}
.second-kill .kill-list{padding:0 30rpx;width:100%;box-sizing:border-box;}
.second-kill .kill-item{justify-content:space-between;align-items:center;height:273rpx;border-bottom:1px dashed #dddddd}
.second-kill .kill-item image{width:200rpx;height:200rpx;border-radius:10rpx;}
.second-kill .kill-item .text{width:460rpx;flex-direction:column;justify-content:space-around;height:200rpx;}
.second-kill .kill-item .text .name{font-size:28rpx;color:#333333;}
.second-kill .kill-item .text .kill-time{width:280rpx;height:46rpx;background-color:#f1f1f1;border-radius:50rpx;color:#aaaaaa;font-size:22rpx;align-items:center;justify-content:center;}
.second-kill .kill-item .text .kill-time .iconfont{margin-right:14rpx;}
.second-kill .kill-item .text .kill-time .time{margin-left:15rpx;color:#333333;font-weight:bold;}
.second-kill .kill-item .text .progress-bar{font-size:20rpx;color:#fa5d4b;}
.second-kill .kill-item .text .progress-bar .kill-bar{position:relative;width:150rpx;height:18rpx;border-radius:50rpx;border:1px solid #fdb0ae;display:inline-block;overflow:hidden;box-sizing:border-box;margin-left:11rpx;}
.second-kill .kill-item .text .progress-bar .kill-bar .kill-fill{position:absolute;height:100%;width:40%;background-color:#fa5d4b;border-radius:50rpx;left:0;top:0;bottom:0;}
.second-kill .kill-item .text .progress{align-items:center;justify-content:space-between;}
.second-kill .kill-item .text .progress .money{font-size:22rpx;color:#fa5d4b;font-weight:bold;}
.second-kill .kill-item .text .progress .money .num{font-size:30rpx;}
.buying-goods .title .icon-tuangoujie{color:#ecc344;border-color:#ecc344;}
.buying-goods .goods-scroll .item{width:280rpx; display: inline-block;margin-left:20rpx;}
.buying-goods .goods-scroll .item .picture{width:100%;height:280rpx;}
.buying-goods .goods-scroll .item .picture image{width:100%;height:100%;border-radius:10rpx 10rpx 0 0;}
.buying-goods .goods-scroll .item .text{height:186rpx;width:100%;background-color:#f9f9f9;text-align:center;font-size:19rpx;border-radius:0 0 10rpx 10rpx;}
.buying-goods .goods-scroll .item .text .name{font-size:24rpx;color:#333333;padding-top:26rpx;}
.buying-goods .goods-scroll .item .text .purchase{color:#999999;margin-top:0.15rem;}
.buying-goods .goods-scroll .item .text .but{width:230rpx;height:46rpx;background-color:#f17e7e;border-radius:50rpx;color:#fff;line-height:46rpx;margin:25rpx auto 0 auto;}
.buying-goods .goods-scroll .item .text .but .iconfont{font-size:30rpx;vertical-align:middle;margin-right:9rpx;}
.buying-goods .goods-scroll{width: 100%; overflow: hidden; white-space: nowrap; padding:20rpx 10rpx 28rpx 10rpx;box-sizing: border-box;}
.like .title{height:120rpx;line-height:120rpx;text-align:center;font-size:26rpx;color:#282828;}
.like .title .iconfont{width:36rpx;height:36rpx;border-radius:100%;border:2rpx solid #f17e7e;font-size:24rpx;display:inline-block;text-align:center;line-height:36rpx;color:#f17e7e;margin-right:17rpx;}
.like .like-list{padding:0 20rpx;flex-wrap:wrap;justify-content:space-between;}
.like .like-list .item{width:347rpx;background-color:#fff;margin-bottom:15rpx;}
.like .like-list .item .picture{width:100%;height:347rpx;}
.like .like-list .item .picture image{width:100%;height:100%;border-radius:10rpx 10rpx 0 0;}
.like .like-list .item .text{height:140rpx;width:100%;padding:0 20rpx;flex-direction:column;justify-content:center;box-sizing:border-box;border-radius:0 0 10rpx 10rpx;}
.like .like-list .item .text .name{font-size:28rpx;color:#000000;}
.like .like-list .item .text .money{font-size:24rpx;color:#fa5d4b;margin-top:15rpx;}
.like .like-list .item .text .money text{font-size:32rpx;}
.empty .picture image{width: 300rpx; height: 300rpx;}
.empty{font-size: 26rpx;text-align: center;color: #f17e7e;}