@import '/app.wxss';
page{height: 100%;}
.newGoodList .item{position:relative;padding:0 30rpx;}
.newGoodList .item .pictrue{width:100%;position:relative;}
.newGoodList .item .pictrue image{width:100%;height:100%;border-radius:6rpx;}
.newGoodList .item .pictrue .numPic{position:absolute;left:7rpx;top:7rpx;width:50rpx;height:50rpx;border-radius:50%;}
.newGoodList .item .underline{padding:30rpx 30rpx 30rpx 0;border-bottom:1px solid #f5f5f5;}
.newGoodList .item:nth-last-child(1) .underline{border-bottom:0; }
.newGoodList .item .text{font-size:30rpx;color:#222;width:489rpx;}
.newGoodList .item .text .money{font-size:26rpx;font-weight:bold;margin-top:10rpx;}
.newGoodList .item .text .money .num{font-size:34rpx;}
.newGoodList .item .text .vip-money{font-size:24rpx;color:#282828;font-weight:bold;margin-top:10rpx;}
.newGoodList .item .text .vip-money .vip{margin-right:22rpx;}
.newGoodList .item .text .vip-money image{width:46rpx;height:21rpx;margin-left:5rpx;}
.newGoodList .item .text .vip-money .num{font-size:22rpx;color:#aaa;font-weight:normal;margin:-2rpx 0 0 0;}
.newGoodList .item .iconfont{position:absolute;right:30rpx;width:50rpx;height:50rpx;border-radius:50%;font-size:30rpx;bottom:38rpx;}